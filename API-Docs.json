                        },
                        "refreshToken": {
                          "type": "string"
                        },
                        "user": {
                          "$ref": "#/components/schemas/User"
                        }
                      }
                    },
                    "errors": {
                      "default": null
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request from client",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
