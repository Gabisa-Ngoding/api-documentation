                  },
                  "password": {
                    "type": "string",
                    "minLength": 8
                  }
                }
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Login Succeed",
            "headers": {
              "Authorization": {
                "schema": {
                  "type": "string",
                  "description": "Bearer {token}"
